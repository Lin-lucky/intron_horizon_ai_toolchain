{
  "inputs": [
    "image"
  ],
  "outputs": [
    "image",
    "face_bbox_list",
    "snap_list",
    "face_feature"
  ],
  "workflow": [
    {
      "thread_count": 3,
      "method_type": "InferMethod",
      "unique_name": "multitask",
      "inputs": [
        "image"
      ],
      "outputs": [
        "body_box",
        "head_box",
        "face_box",
        "lmk",
        "pose",
        "kps"
      ],
      "method_config_file": "infer_multitask.json"
    },
    {
      "thread_count": 1,
      "method_type": "MOTMethod",
      "unique_name": "face_mot",
      "inputs": [
        "face_box"
      ],
      "outputs": [
        "face_bbox_list",
        "face_disappeared_track_id_list"
      ],
      "method_config_file": "iou_method_param.json"
    },
    {
      "method_type": "FaceSnapFilterMethod",
      "unique_name": "filter",
      "inputs": [
        "face_bbox_list",
        "pose",
        "lmk"
      ],
      "outputs": [
        "filter_info",
        "rgb_bbox_list_final",
        "pose_after_filter",
        "lmk_after_filter"
      ],
      "method_config_file": "snapshot_filter_pic.json"
    },
    {
      "method_type": "GradingMethod",
      "unique_name": "grading",
      "inputs": [
        "rgb_bbox_list_final",
        "pose_after_filter",
        "lmk_after_filter"
      ],
      "outputs": [
        "select_score_list"
      ],
      "method_config_file": "grading.json"
    },
    {
      "method_type": "SnapShotMethod",
      "unique_name": "snapshot",
      "inputs": [
        "image",
        "rgb_bbox_list_final",
        "select_score_list",
        "face_disappeared_track_id_list",
        "pose_after_filter",
        "lmk_after_filter"
      ],
      "outputs": [
        "snap_list"
      ],
      "method_config_file": "snapshot_pic.json"
    },
    {
      "method_type": "InferMethod",
      "unique_name": "feature",
      "inputs": [
        "snap_list"
      ],
      "outputs": [
        "face_feature"
      ],
      "method_config_file": "infer_feature.json"
    }
  ]
}
