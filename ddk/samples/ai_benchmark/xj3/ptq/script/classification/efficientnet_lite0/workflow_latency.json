{
  "input_config": {
    "input_type": "image",
    "height": 224,
    "width": 224,
    "data_type": 1,
    "image_list_file": "../../../mini_data/imagenet/imagenet.lst",
    "need_pre_load": true,
    "limit": 1,
    "need_loop": true,
    "max_cache": 10
  },
  "output_config": {
    "output_type": "image",
    "image_list_enable": true,
    "image_output_dir": "./output_images"
  },
  "inputs": [
    "input_data"
  ],
  "outputs": [
    "input_data",
    "perception_data"
  ],
  "workflow": [
    {
      "thread_count": 1,
      "method_type": "InferMethod",
      "unique_name": "InferMethod",
      "inputs": [
        "input_data"
      ],
      "outputs": [
        "tensors"
      ],
      "method_config": {
        "core": 0,
        "model_file": "../../../model/runtime/efficientnet_lite0/efficientnet_lite0_224x224_nv12.bin"
      }
    },
    {
      "thread_count": 1,
      "method_type": "PTQClassficationPostProcessMethod",
      "unique_name": "PTQClassficationPostProcessMethod",
      "inputs": [
        "input_data",
        "tensors"
      ],
      "outputs": [
        "perception_data"
      ],
      "method_config": {
        "top_k": 1,
        "cls_names_list": "../../config/data_name_list/imagenet.list"
      }
    }
  ]
}
